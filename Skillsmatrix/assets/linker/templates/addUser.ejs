					<tr data-id="<%= user.id %>" data-model="user"> 
 						<td>
 							<% if(user.online) { %>
	 							<div id="table_user_status" class="online">
	 								<span class="entypo-star green"></span>
								</div>
							<% } else { %>
	 							<div id="table_user_status" class="offline">
	 								<span class="entypo-star-empty red"></span>
								</div>
							<% } %>
 						</td>
 						<td><%= user.id %></td>
						<td><a href="/user/show/<%= user.id %>"><%= user.name %></a></td>
						<td><%= user.title %></td>
						<td><%= user.email %></td>
						<td class="user_rights_icon">
							<% if(user.admin) { %>
								Admin
							<% } else if(user.manager) { %>
								Manager
							<% } else { %>
								Employee
							<% } %>
						</td>
						<td><a href="/user/show/<%= user.id %>" class="btn btn-sm btn-primary">Show</a></td>
						<td><a href="/user/edit/<%= user.id %>" class="btn btn-sm btn-warning">Edit</a></td>
						<td>
							<form action="/user/destroy/<%= user.id %>" method="POST">
								<input type="hidden" name="_method" value="delete">
								<input type="submit" class="btn btn-sm btn-danger" value="Delete">
								<input type="hidden" name="_csrf" value="<%= _csrf %>">
							</form>
						</td>
					</tr>